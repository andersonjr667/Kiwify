PORT=3000
# Token secreto enviado no header x-webhook-token
KIWIFY_WEBHOOK_TOKEN=coloque-um-token-forte
# Segredo para assinatura HMAC SHA256 do corpo do webhook
KIWIFY_WEBHOOK_SECRET=coloque-um-segredo
# Header onde a assinatura é enviada (padrão: x-kiwify-signature)
KIWIFY_SIGNATURE_HEADER=x-kiwify-signature
# Evento esperado do Kiwify (padrão: order_paid)
KIWIFY_EVENT_NAME=order_paid
# Status considerado pago (padrão: paid)
KIWIFY_PAID_STATUS=paid
